{
    "executionRoleArn": "arn:aws:iam::060274181246:role/dns-ecs",
    "containerDefinitions": [
      {
        "name": "backend",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort"     : 80
          }
        ],
        "healthCheck": {
          "command" : [ "CMD-SHELL", "curl -f http://localhost:80/docs || exit 1" ]
        }
      }
    ],
    "placementConstraints": [],
    "memory": "512",
    "taskRoleArn": "arn:aws:iam::060274181246:role/dns-ecs",
    "family": "backend",
    "networkMode": "awsvpc",
    "cpu": "256"
  }